          </Anchor>
        </Anchors>
		<Scripts>
			<OnLoad>
				OnlineFrame.scrollFrame = self;
			</OnLoad>
			<OnMouseWheel>
				local at = OnlineFrame.slider:GetValue();
				if (arg1 == 1) then
					OnlineFrame.slider:SetValue(at + (-1) * OnlineFrame.slider:GetValueStep());
				elseif (arg1 == -1) then
					OnlineFrame.slider:SetValue(at + OnlineFrame.slider:GetValueStep());
				end
			</OnMouseWheel>
		</Scripts>
        <Frames>
